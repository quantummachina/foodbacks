<% provide(:title, 'Update this Foodback') %>
<h1>Edita tu Foodback</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@foodback) do |f| %>
      <%= render 'shared/error_messages', object: @foodback %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :invited %>
      <%= f.text_field :invited %>

      <%= f.label :address %>
      <%= f.text_field :address %>

      <%= f.label :content %>
      <%= f.text_field :content %>

      <%= f.label :dish %>
      <%= f.text_field :dish %>

      <%= f.submit "Update this Foodback", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>